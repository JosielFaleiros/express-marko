<include("./header.marko")/>
<h1>Cidades</h1>
<table>
  <tr>
    <th>Nome</th>
    <th>Estado</th>
  </tr>
  <tr for(cidade in data.cidades)>
    <td>${cidade.nome}</td>
    <td>${cidade.estado}</td>
  </tr>
</table>
<hr>
<form action="/cidade" method="POST">
  Nome: <input type="text" name="nome" required> <br>
  Estado: <input type="text" name="estado" required> <br>
  <input type="submit" value="Submit">
</form>
